<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.kailink.ui.home.HomeFragment"
        android:elevation="8dp"
        android:label="마이페이지"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_contacts"
        android:name="com.example.kailink.ui.contacts.ContactsFragment"
        android:elevation="8dp"
        android:label="연락처"
        tools:layout="@layout/fragment_contacts"
        />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.kailink.ui.gallery.GalleryFragment"
        android:elevation="8dp"
        android:label="갤러리"
        tools:layout="@layout/fragment_gallery"
        />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.kailink.ui.chats.ChatsFragment"
        android:elevation="8dp"
        android:label="넙죽이봇"
        tools:layout="@layout/fragment_chats" />
</navigation>